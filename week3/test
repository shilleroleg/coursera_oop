if __name__ == "__main__":
    achieve = {"title": "Покоритель", "text": "Дается при выполнении всех заданий в игре"}

    achievements = set()

    print(achieve.get('title'))
    achievements.add(achieve.get('title'))

    print(achievements)


    ach2 = dict()

    ach2.update(achieve)

    print(ach2)

